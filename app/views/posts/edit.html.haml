.col-md-6.col-md-offset-3
  .row
    .panel.panel-default
      .panel-heading
        %h1 Editing post
      .panel-body
        - if @post.image.present? 
          = image_tag @post.image.url(:medium) 
        = render 'form' 
        .span.glyphicon.glyphicon-arrow-left
        = link_to "Back", :back
            
        

	
	
	
	